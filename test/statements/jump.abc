// range
class range {
    // init
    init(begin: int, end: int)
        : begin{begin}, end{end}

    // begin(), end()
    func begin() -> int {
        return begin;
    }
    func end() -> int {
        return end;
    }

    // begin, end    
    var begin: int;
    var end: int;
}

func main() -> int {
    // jump in for
    for i in range(0, 10) {
        if i == 0 {
            skip;
        }
        elif i == 5 {
            break;
        }
        std::print(i);
    }
    // jump in while
    let i := 10;
    while i-- > 0 {
        if i == 10 {
            skip;
        }
        elif i == 5 {
            break;
        }
        std::print(i);
    }
    // jump in func
    return 0;
}